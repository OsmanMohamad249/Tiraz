# CI Environment Variables Template
# Copy this file to .ci.env and fill in the values

# Database Configuration
POSTGRES_USER=qeyafa_test
POSTGRES_PASSWORD=your_test_password_here
POSTGRES_DB=qeyafa_test
DATABASE_URL=postgresql://qeyafa_test:your_test_password_here@db:5432/qeyafa_test

# Backend Configuration
SECRET_KEY=your_secret_key_here_min_32_chars
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# AI Service Configuration
OPENAI_API_KEY=your_openai_key_here
AI_SERVICE_URL=http://ai-service:8001
AI_SERVICE_TIMEOUT=30

# Storage Configuration
STORAGE_TYPE=local
UPLOAD_DIR=/app/uploads

# CORS Configuration
CORS_ORIGINS=["http://localhost:3000","http://localhost:8080"]

# Environment
ENVIRONMENT=test
DEBUG=false

# Logging
LOG_LEVEL=INFO
